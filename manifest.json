{
  "name": "learnmutiny",
  "version": "0.1.0",
  "description": "learnmutiny browser extension",
  "permissions": 
      [
        "storage", 
        "tabs", 
        "declarativeNetRequest",  
        "geolocation", 
        "mdns", 
        "proxy"
      ],
  "host_permissions":  
      [
        "https://*.youtube.com/*",
        "https://*.w3schools.com/*"
      ],
  "background": 
      {
        "service_worker": "background.js"
      },
  "content_scripts": 
    [
      {
        "matches": 
          [
            "https://*.youtube.com/*",
            "https://*.w3schools.com/*"
          ],
        "js": ["contentScript.js"]
      }
    ],
  "web_accessible_resources": 
    [
      {
        "resources": [
          "assets/bookmark.png",
          "assets/play.png",
          "assets/delete.png",
          "assets/save.png",
          "assets/bear.png"
        ],
        "matches": [
          "https://*.youtube.com/*",
          "https://*.w3schools.com/*"
        ]
      }
    ],
  "action": 
    {
      "default_icon": 
        {
          "16": "assets/bear2.png",
          "24": "assets/bear2.png",
          "32": "assets/bear2.png"
        },
      "default_title": "learnmutiny extension",
      "default_popup": "extension.html"
    },
  "manifest_version": 3
}
